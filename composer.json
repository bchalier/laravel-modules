{
    "name": "bchalier/laravel-modules",
    "description": "Module management in laravel.",
    "keywords": [
        "laravel",
        "modules",
        "module",
        "bchalier",
        "split",
        "package"
    ],
    "license": "MIT",
    "authors": [
        {"name": "Bastien Chalier", "email": "bastien@chalier.io"}
    ],
    "require": {
        "php": ">=7.1",
        "wikimedia/composer-merge-plugin": "^1.4",
        "localheinz\/json-printer": "^2.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Bchalier\\Modules\\": "src/",
            "SystemModules\\": "src/SystemModules/",
            "Modules\\": "../../../modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bchalier\\Modules\\LaravelModulesServiceProvider"
            ],
            "aliases": {
                "ModulesManager": "SystemModules\\Core\\Facades\\ModulesManager"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
